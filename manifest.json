{  
    "name":"Jira Extension - DFDW",
    "version":"0.1.0",
    "permissions":[
       "activeTab",
       "tabs",
       "storage"
    ],
    "options_ui": {
       "page": "options.html",
       "open_in_tab": true,
       "browser_style": true
    },
    "action": {
       "default_popup": "popup.html",
       "default_icon": {
          "16":"icons/icon16-dark.png",
          "48":"icons/icon48-dark.png",
          "128":"icons/icon128-dark.png"
       }
    },
    "manifest_version":3
 }